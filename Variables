#declaring the variables

trainee=Karthik
class=Devops

echo trainer name = $trainer
echo class name = $class
echo class name = ${class}

# command to check the date in linux
Date=$(date +%F)
echo hey, Today date is $DATE


ADD=$((2+3))
echo ADD - $ADD

#Properties of the variable. Default type is Read Write

a = 10
echo $a
10

a=20
echo $a
20

#Read only
a=100
readonly a
a=200






